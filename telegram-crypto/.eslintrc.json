{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "off",
        "indent" : ["error", 4],
        "object-curly-spacing": ["error", "always"],
        "no-prototype-builtins": "off",
        "quotes":"off"
    }
}
